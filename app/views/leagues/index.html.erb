<h1> Listing All League </h1>
<%= link_to "Create new league", new_league_path %>


<table>
	
<tr>
	
<th>Name</th>

</tr>


<% @leagues.each do |league| %>

<tr>

	<td><%= league.name %> </td>
	<td><%= link_to "Edit", edit_league_path(league) %></td>
	<td><%= link_to "Show", league_path(league) %></td>
	<td><%= link_to "Delete", league_path(league), method: :delete, data:{confirm: "Are you sure?"} %></td>

</tr>
<% end %>


</table>