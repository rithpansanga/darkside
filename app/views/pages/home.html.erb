<h1> Listing all teams</h1>


<table>
	<tr>
	   
	   <th>Home</th>
	   <th>Score</th>
	   <th>Away</th>
	</tr>

	<% @Matches.each do |match| %>

	<tr>
	  <td><%= match.home.team.name %></td>
	  <td><%= match.result %></td>
	  <td><%= match.away.team.name %></td>
	 
	  
	<% end %>
	
	
	

	
	</tr>
	
	
	
	  




</table>
